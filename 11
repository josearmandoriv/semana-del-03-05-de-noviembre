#include <stdio.h>

main()
{
	float x;
	int sum,res,mul,div;
	printf("Evaluacion de expresiones \n");
	
	x=5/2+20 % 6;
	printf("el resultado es: %f \n", x);
	x=4*6/2-15/2;
	printf("el resultado es: %f \n", x);
	x=5*15/2/(4-2);
	printf("el resultado es: %f \n", x);
	x=8==16||7!= 4 && 4<1;
	printf("el resultado es: %f \n", x);
	x=(4*3<6 || 3>5-2) && 3+2<12;
	printf("el resultado es: %f \n", x);
	x=7+3*6/2-1;
	printf("el resultado es: %f \n", x);
	x=2 % 2+2*2-2/2;
	printf("el resultado es: %f \n", x);
	x=(3*9*(3+(9*3/(3))));
	printf("el resultado es: %f \n", x);
}
