#include <stdio.h>

main()
{
	int a, b, c;
	int x,x1;
	printf("ecuaciones de primer grado \n");
	printf("introduce el valor de a: \n");
	scanf("%d", &a);
	printf("introduce el valor de b: \n");
	scanf("%d", &b);
	printf("introduce el valor de c: \n");
	scanf("%d", &c);
	
	x1=c-b;
	x=x1/a;
	printf("el valor de x es: %d",x);
}
